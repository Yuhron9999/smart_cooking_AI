{
  "name": "frontend-clean",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "next dev --port 3000",
    "build": "next build",
    "build:clean": "rimraf .next && next build",
    "start": "next start",
    "lint": "next lint",
    "fix:hydration": "echo Checking for hydration issues... && findstr /C:toLocaleString pages\\*.tsx && echo Found toLocaleString usage - use SafeStats instead || echo No hydration issues found",
    "test:css": "start http://localhost:3004/css-test && next dev --port 3004",
    "clean": "rimraf .next out node_modules\\.cache",
    "clean:all": "rimraf .next out node_modules && npm install",
    "reset": "rimraf .next && npm run dev"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.536.0",
    "next": "14.2.30",
    "next-auth": "^4.24.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.30",
    "postcss": "^8.5.6",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.13",
    "typescript": "^5"
  }
}
